	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6B8A99"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF6B88"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator.assignment",
				"keyword.operator.logical",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ff5577"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#5dd5ff"
			}
